<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Salvar_Servlet</servlet-name>
        <servlet-class>clientes.Salvar_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>clientes.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExcluirServlet</servlet-name>
        <servlet-class>clientes.ExcluirServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarServlet</servlet-name>
        <servlet-class>clientes.EditarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Salvar_Historico_Servlet</servlet-name>
        <servlet-class>historico.Salvar_Historico_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editar_Historico_Servlet</servlet-name>
        <servlet-class>historico.Editar_Historico_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Excluir_Historico_Servlet</servlet-name>
        <servlet-class>historico.Excluir_Historico_Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Editar_Servlet</servlet-name>
        <servlet-class>cidades.Editar_Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Salvar_Servlet</servlet-name>
        <url-pattern>/Salvar_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExcluirServlet</servlet-name>
        <url-pattern>/ExcluirServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarServlet</servlet-name>
        <url-pattern>/EditarServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Salvar_Historico_Servlet</servlet-name>
        <url-pattern>/Salvar_Historico_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editar_Historico_Servlet</servlet-name>
        <url-pattern>/Editar_Historico_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Excluir_Historico_Servlet</servlet-name>
        <url-pattern>/Excluir_Historico_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Editar_Servlet</servlet-name>
        <url-pattern>/Editar_Servlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
